<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TimePicker">

        <!-- Initial value in minutes since the beginning of the day (24 hour format) -->
        <attr name="initValue" format="integer" />

        <!--  Defines the initial value -->
        <attr name="initValueMode">
            <!-- Default values -->
            <enum name="defaultValue" value="1" />
            <!-- The value specified by initValues -->
            <enum name="value" value="2" />
            <!-- Current time -->
            <enum name="currentTime" value="3" />
        </attr>
    </declare-styleable>

</resources>